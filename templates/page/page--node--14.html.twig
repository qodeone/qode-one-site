    </div>

  </div>
</div>
</div>
<!-- clouds content end -->
   
        
          <section class="section uk-text-center">
          <div class="uk-contrast uk-text-large">
            {{ page.pagetitle }}
            </div>
            {{ page.content }}
          </section>
        </main>
                    
                    
     </div>                  
                    


  </div>   





                    
</div> <!--/ .uk-grid /-->
</div> <!--/ .uk-container /-->

